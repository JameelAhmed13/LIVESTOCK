@model IEnumerable<LIVESTOCK.Areas.CovidLab.Models.Patient>



<table class="table table-striped table-bordered  dataTables-example">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FatherHusbandName)
            </th>
            <th>
                <text>Mobile No</text>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                <text>CNIC</text>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profession)
            </th>
            @*<th>
            @Html.DisplayNameFor(model => model.Address)
        </th>*@
            <th>
                @Html.DisplayNameFor(model => model.DOS)
            </th>
            @*<th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>*@
            <th>
                @Html.DisplayNameFor(model => model.District)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FatherHusbandName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CNIC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Profession)
            </td>
            @*<td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.DOS)
            </td>
            @*<td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.District.DistrictName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PatientID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PatientID">Details</a> @*|
                <a asp-action="Delete" asp-route-id="@item.PatientID">Delete</a>*@
            </td>
        </tr>
        }
    </tbody>
</table>

<script src="~/libs/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/libs/datatables/js/dataTables.bootstrap.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.dataTables-example').DataTable({
        });
    });
</script>